@model jjs.Models.Menu

@{
    ViewBag.Title = "Delete";
}

<h2>删除</h2>

<h3>是否确定删除?</h3>
<p style="float:right">
    @Html.ActionLink("返回列表", "Index")
</p>
<div>
    <h4>菜单</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.MenuTitle)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MenuTitle)
        </dd>

        <dt>
            菜单链接
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MenuLink)
        </dd>

        <dt>
            菜单级别
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MenuLevel)
        </dd>

        <dt>
            父菜单
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ParentId)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.InDateTime)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.InDateTime)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="删除" class="btn btn-default" />
        </div>
    }
</div>
